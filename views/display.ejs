<%- include('./includes/head.ejs'); %>
    <title><%=title %></title>
</head>

<body>
    <%- include('./includes/navbar.ejs'); %>
    <% for(let u of users) { %>
    <div class="card text-white bg-info m-3" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title"><%=u.name %></h5>
            <h6 class="card-subtitle mb-2 text-muted"><%=u.email %></h6>
            <p class="card-text"><%=u._id %></p>
            <a href="edit/<%=u.id %>" class="card-link btn btn-warning">Update</a>
            <a href="#" class="card-link btn btn-danger">Delete</a>
        </div>
    </div>
    <% } %>
<%- include('./includes/end.ejs'); %>